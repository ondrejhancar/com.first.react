# Editor configuration, see http://editorconfig.org
# Java code style is managed by configuration of IntelliJ IDEA

root = true

[{*.js,*.gradle,*.md}]
indent_style = space
indent_size = 2
charset = utf-8
insert_final_newline = true
trim_trailing_whitespace = true

[*.md]
max_line_length = off
trim_trailing_whitespace = false
